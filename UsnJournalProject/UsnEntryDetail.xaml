<!--UsnEntryDetail.xaml-->
<Window x:Class="UsnJournalProject.UsnEntryDetail"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    UseLayoutRounding="True" RenderOptions.BitmapScalingMode="Fant"
    Title="USN Entry Detail" SizeToContent="WidthAndHeight" WindowStartupLocation="Manual" ShowInTaskbar="False">
    <StackPanel>
      <TextBlock Name="_nameLbl" MouseLeftButtonUp="_nameLbl_MouseDoubleClick" TextWrapping="Wrap"/>
      <TextBlock Name="_pathLbl" MouseLeftButtonUp="_nameLbl_MouseDoubleClick" TextWrapping="Wrap"/>
        <Label Name="_entryDetailLbl" />
    </StackPanel>
</Window>
